<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Tiêu đề</th>
            <th style="width:150px">Ngày đăng</th>
            <th style="width:150px">Người đăng</th>
            <th style="width:50px">Chọn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var it in Model.ListTopic)
        {
        <tr>

            <td>@it.Name</td>
            <td>@it.CreateDate.ToString("hh:mm dd/MM/yyyy")</td>
            <td>@AppHelpers.GetMembersAccount(it.MembershipUser_Id)</td>
            <td>
                <span>
                    <a class="btn-mvc" onclick="POPUPSELECT.Select('@it.Id','@it.Name')">
                        <span class="glyphicon glyphicon-saved"></span>
                    </a>
                </span>
            </td>
        </tr>
        }
    </tbody>
</table>